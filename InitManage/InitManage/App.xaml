<?xml version = "1.0" encoding = "UTF-8" ?>
<Application xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:InitManage"
             xmlns:Converters="clr-namespace:InitManage.Commons.Converters"
             x:Class="InitManage.App">
    <Application.Resources>
        <ResourceDictionary>
            <Converters:BoolToPrimaryColorValueConverter x:Key="BoolToPrimaryColorValueConverter"/>
            <Converters:ListToStringValueConverter x:Key="ListToStringValueConverter"/>

            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Styles/Colors.xaml" />
                <ResourceDictionary Source="Resources/Styles/Styles.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>

