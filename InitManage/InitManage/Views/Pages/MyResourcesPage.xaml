<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="InitManage.Views.Pages.MyResourcesPage"
             IconImageSource="opened_book">
    <Grid RowDefinitions="*">
        <StackLayout Margin="20">
            <Label
                Text="Mes réservations"
                FontSize="35"
                TextColor="{StaticResource Primary}"
                FontAttributes="Bold"/>
        </StackLayout>

        <Ellipse
            VerticalOptions="End"
            HorizontalOptions="End"
            Margin="160"
            Grid.Row="1"
            HeightRequest="50"
            WidthRequest="50"/>

        <Ellipse
            VerticalOptions="End"
            HorizontalOptions="End"
            Margin="70"
            Grid.Row="1"
            HeightRequest="100"
            WidthRequest="100"/>

        <Ellipse
            Margin="-90"
            Grid.Row="1"
            VerticalOptions="End"
            HorizontalOptions="End"
            HeightRequest="200"
            WidthRequest="200"/>
    </Grid>
</ContentPage>
